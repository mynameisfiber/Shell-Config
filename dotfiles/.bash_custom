if [[ $TERM == "screen" ]]
  then
    export PS1="${debian_chroot:+($debian_chroot)}\u@\h:\w\$ "
  fi
#



if [ -f ~/.bash_github ]; then 
   . ~/.bash_github
  fi

export PATH="/usr/local/bin:$PATH"

# VI mode!
set -o vi
export EDITOR="vim"


for script in ~/.bash_scripts/*;
do
    if [ -r "$script" ]; then
        source "$script"
    fi
done

source ~/.profile_custom
